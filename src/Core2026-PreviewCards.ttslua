local GUIDReferenceApi = require("core/GUIDReferenceApi")

function onLoad()
  local AdditionalPlayerCardsBag = GUIDReferenceApi.getObjectByOwnerAndType("Mythos", "AdditionalPlayerCardsBag")
  if AdditionalPlayerCardsBag then
    broadcastToAll("Adding Core 2026 preview cards to index...", "Green")
    AdditionalPlayerCardsBag.putObject(self)
  end
end
